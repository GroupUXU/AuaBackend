{
  "kind": "collectionType",
  "collectionName": "leads",
  "info": {
    "singularName": "lead",
    "pluralName": "leads",
    "displayName": "lead",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "secondName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "recid": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "new",
        "waitForContract",
        "rejected",
        "recontact",
        "atAnalysis",
        "atLawyer",
        "inCourt",
        "wonInCourt",
        "lostInCourt",
        "nowy",
        "odrzucony",
        "zakonczony ",
        "analiza",
        "umowa",
        "dokumentacja",
        "prawnik"
      ],
      "default": "new",
      "required": true
    },
    "pesel": {
      "type": "string"
    },
    "planowanyKontakt": {
      "type": "dynamiczone",
      "components": [
        "others.planowany-kontakt"
      ],
      "required": true
    },
    "kredyty": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::kredyty.kredyty",
      "mappedBy": "lead"
    }
  }
}
